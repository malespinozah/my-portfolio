{
    "version": 2,
    "builds": [
      {
        "src": "react-portfolio/package.json",
        "use": "@vercel/static-build"
      },
      {
        "src": "server/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/server/index.js"
      },
      {
        "src": "/(.*)",
        "dest": "/react-portfolio/build/$1"
      }
    ]
  }
  