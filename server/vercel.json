{
  "version": 2,
  "builds": [
    {
      "src": "server/api/skills.js",
      "use": "@vercel/node"
    },
    {
      "src": "server/api/projects.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/skills",
      "dest": "server/api/skills.js"
    },
    {
      "src": "/api/projects",
      "dest": "server/api/projects.js"
    }
  ]
}
